export type Permutations2<A, B> = A extends string
  ? B extends string
    ? A | B | `${A}, ${B}`
    : A
  : never;

export type Permutations3<A, B, C> = A extends string
  ? B extends string
    ? C extends string
      ? Permutations2<A, B> | Permutations2<B, C> | `${A}, ${B}, ${C}`
      : Permutations2<A, B>
    : A
  : never;
